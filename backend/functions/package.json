{
  "name": "functions",
  "version": "0.0.1",
  "private": true,
  "description": "QX Analytics firebase functions based on Typescript + Express API server",
  "author": "Jon<jonbak.dev@gmail.com>",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "deploy": "firebase deploy --only functions",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "logs": "firebase functions:log",
    "serve": "npm run build && npm run start",
    "serve:watch": "concurrently --kill-others \"npm run build:watch\" \"npm run start\"",
    "shell": "npm run build && firebase functions:shell",
    "start": "firebase emulators:start --import $INIT_CWD/.emulators --export-on-exit $INIT_CWD/.emulators",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@cowprotocol/ts-dune-client": "^0.0.2",
    "@google-cloud/bigquery": "^7.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.7.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jszip": "^3.10.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "onebrick-node-sdk": "^1.0.17",
    "openai": "^4.28.0",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "passport-oauth2-refresh": "^2.2.0",
    "puppeteer": "22.1.0",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsc-alias": "^1.8.8",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node-fetch": "^2.6.11",
    "@types/node-forge": "^1.3.11",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "@types/passport-oauth2-refresh": "^1.1.4",
    "@types/puppeteer": "^5.4.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "firebase-functions-test": "^3.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "uuidv4": "^6.2.13"
  }
}
